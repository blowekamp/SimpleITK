//
// Function to run the Execute method of this filter
//
Image ${name} ( $(include ImageParameters.in)$(when members $(foreach members ,
  ${type} in${name})) )
{
  ${name}ImageFilter filter;
  return filter.Execute ( $(for inum=1,number_of_inputs do
                              if inum>1 then
                                OUT=OUT..', '
                              end
                              OUT=OUT .. 'image' .. inum
                            end)$(when members $(foreach members , in${name})) );
}
